package api

import (
	"github.com/riccardotornesello/irapi-go/client"
	{% for category in categories %}"github.com/riccardotornesello/irapi-go/api/{{category["package_name"]}}"
    {% endfor %}
)

type IRacingApi struct {
    {% for category in categories %}{{category["camel_category"]}} *{{category["package_name"]}}.{{category["struct_name"]}}
    {% endfor %}
}

func NewIRacingApi(apiClient *client.ApiClient) *IRacingApi {
	return &IRacingApi{
        {% for category in categories %}{{category["camel_category"]}}: &{{category["package_name"]}}.{{category["struct_name"]}}{Client: apiClient},
        {% endfor %}
	}
}
